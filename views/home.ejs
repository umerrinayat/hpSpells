<!-- Third (Works) section -->
<section class="section" id="themes">
	<div class="container">
	
		<h2 class="text-center title whiteText">Magic Spells</h2>
		<p class="lead text-center whiteText">
			Huge thanks to corpora who publish<br>
			this data on github, thank you!
		</p>
<!-- Here to put ejs for function -->
	<% for(var i=0; i < spells.length - 1; i = i+2){ %>
	
		<div class="row">
			<div class="col-sm-4 col-sm-offset-2">
				<div class="thumbnail">
					<div class="caption">
						<h3><%= spells[i].incantation %></h3>
						<p><%= spells[i].effect %>.</p>
						<p><a href="#" class="btn btn-primary" role="button">Type</a> <a href="#" class="btn btn-default" role="button"><%= spells[i].type %></a></p>
					</div>
				</div>
			</div>
			<div class="col-sm-4">
				<div class="thumbnail">
					
					<div class="caption">
						<h3><%= spells[i+1].incantation %></h3>
						<p><%= spells[i+1].effect %>.</p>
						<p><a href="#" class="btn btn-primary" role="button">Type</a> <a href="#" class="btn btn-default" role="button"><%= spells[i+1].type %></a></p>
					</div>
				</div>
			</div>

			
	<!-- Till here -->	
		</div>
		<% } %>

	</div>
</section>

